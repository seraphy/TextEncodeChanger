<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jp.seraphyware.textencodechanger.ui.TextPreviewController">
   <center>
      <TextArea fx:id="textArea" editable="false" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <font>
            <Font size="14.0" />
         </font></TextArea>
   </center>
   <bottom>
      <HBox alignment="CENTER_RIGHT" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnOK" cancelButton="true" defaultButton="true" mnemonicParsing="false" text="%btn.OK" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="25.0" top="5.0" />
         </padding>
      </HBox>
   </bottom>
   <top>
      <BorderPane BorderPane.alignment="CENTER">
         <center>
            <Label fx:id="lblFileName" text="lblFileName" textOverrun="CENTER_ELLIPSIS" BorderPane.alignment="CENTER_LEFT">
               <font>
                  <Font size="14.0" />
               </font></Label>
         </center>
         <right>
            <Label fx:id="lblEncoding" text="lblEncoding" textFill="BLUE" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </BorderPane.margin></Label>
         </right>
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
      </BorderPane>
   </top>
</BorderPane>
